        WHERE t.status = 'ativa'
     AND d.nome = :dia AND UPPER(tt.nome) LIKE UPPER(:tipo)
        GROUP BY t.id, t.nome, i.nome, d.nome, t.vagas_total, tt.nome, p.nome, t.ultima_presenca_em
        ORDER BY 
            CASE tt.nome 
                WHEN 'MULTIMÍDIA' THEN 1 
                WHEN 'DINÂMICO' THEN 2 
                ELSE 3 
            END,
            CASE d.nome
                WHEN 'Segunda' THEN 1
                WHEN 'Terça' THEN 2  
                WHEN 'Quarta' THEN 3
                WHEN 'Quinta' THEN 4
                WHEN 'Sexta' THEN 5
                WHEN 'Sábado' THEN 6
                WHEN 'Domingo' THEN 7
                ELSE 8
            END,
            t.nome
    
